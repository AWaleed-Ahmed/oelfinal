<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="controller.SceneController" stylesheets="styles.css">
    <Label layoutX="100" layoutY="30" text="Reviews"/>
    <ListView fx:id="reviewList" layoutX="100" layoutY="70" prefHeight="200" prefWidth="400"/>
    <Button layoutX="100" layoutY="290" text="Back to Product" onAction="#goToProductDetails"/>
</AnchorPane>
